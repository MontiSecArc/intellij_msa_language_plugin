<?xml version='1.0' encoding='UTF-8'?>

<templateSet group="MSA">
    <template name="trustlevel" description="trustlevel definition" toReformat="false" toShortenFQNames="true"
              value="trustlevel $LEVEL$ '$DESCRIPTION$';$END$">

        <variable name="LEVEL" expression="" defaultValue="&quot;+1&quot;" alwaysStopAt="true"/>
        <variable name="DESCRIPTION" expression="" defaultValue="&quot;description&quot;" alwaysStopAt="true"/>

        <context>
            <option name="MSA_COMPONENT_BODY" value="true" />
        </context>
    </template>
</templateSet>