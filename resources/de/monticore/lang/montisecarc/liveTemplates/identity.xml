<?xml version='1.0' encoding='UTF-8'?>

<templateSet group="MSA">
    <template name="identity" description="identity link" toReformat="false" toShortenFQNames="true"
              value="identity $TYPE$ $SOURCE$ -> $TARGET$;$END$">

        <variable name="TYPE" expression="enum(weak,strong)" defaultValue="&quot;weak&quot;" alwaysStopAt="true"/>
        <variable name="SOURCE" expression="" defaultValue="&quot;source&quot;" alwaysStopAt="true"/>
        <variable name="TARGET" expression="" defaultValue="&quot;target&quot;" alwaysStopAt="true"/>

        <context>
            <option name="MSA_COMPONENT_BODY" value="true" />
        </context>
    </template>
</templateSet>